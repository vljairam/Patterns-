Code : 
//
import java .util.*;
class main {
    public static void main(String args[]){
        Scanner s = new Scanner(System.in);
        int num = s.nextInt();
        for(int i=0;i<num;i++){
            for(int j=num;j>i;j--){
                System.out.print(num-j+1+" ");
            }
            System.out.println();
        }
    }
}
