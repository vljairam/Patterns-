Code :
//
import java .util.*;
class main {
    public static void main(String args[]){
        Scanner s = new Scanner(System.in);
        int num = s.nextInt();
        for(int i=0;i<=2*num-1;i++){
            int stars = i;
            if(i>num) stars= 2*num-i;
            for(int j=1;j<=stars;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
