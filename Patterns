Code : 
//
import java .util.*;
class main {
    public static void main(String args[]){
        Scanner s = new Scanner(System.in);
        int num = s.nextInt();
        for(int i=0;i<num;i++){
            for(int j=0;j<num-i+1;j++){
                System.out.print(" ");
            }
            for(int j=0;j<2*i+1;j++){
                System.out.print("*");
            }
            for(int j=0;j<num-i+1;j++){
                System.out.print(" ");
            }
            System.out.println();
        }
    }
}
